<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping>
	<class name="com.pragmatic.bookself.student.StudentEntity"
		table="student">
		<meta attribute="class-description">
		This class contains the student detail.
		</meta>
		<id name="id" type="int" column="id">
			<generator class="increment" />
		</id>
		<property name="s_name" column="s_name" type="string" />
		<property name="stream" column="stream" type="string" />
			<property name="semester" column="semester" type="string" />
		<property name="id_issue_book" column="id_issue_book" type="int" />
		<property name="book_issue_date" column="book_issue_date" type="date" />
		<property name="book_due_date" column="book_due_date" type="date" />
	</class>
</hibernate-mapping>